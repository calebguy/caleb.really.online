generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserSetting {
  id        Int      @id @default(autoincrement())
  userId   Int?
  isOnline Boolean?
  users     User?   @relation(fields: [userId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model User {
  id            Int             @id @default(autoincrement())
  name          String?
  UserSettings UserSetting[]
}
